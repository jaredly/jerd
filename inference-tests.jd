
// Inference tests!
// The test passes if the body of the term gets inferred to be the
// type that it's annotated as.
// Inference in this case does *not* use the toplevel annotation,
// which is normally used.

const x: int = 4
const y: string = "hello"
const z: (int) => string = (x: int): string => "hi"

const simpleFn: (int) => int = (x) => x + 5

const usesDeclared: (int) => int = (x) => simpleFn(x)

const recursive: (int) => int = (x) => recursive(x + 1) + 2

